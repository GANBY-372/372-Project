                        <TableColumn fx:id="acquisitionDateColumn" minWidth="0.0" prefWidth="159.0" text="Acquisition Date" />
                        <TableColumn fx:id="isRentableColumn" minWidth="0.0" prefWidth="159.0" text="Rentable" />
                    </columns>
                  </TableView>
