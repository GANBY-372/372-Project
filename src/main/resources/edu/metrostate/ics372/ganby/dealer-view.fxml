                            text="Sport Car"
                            wrapText="true"
                        >
                           <font><Font size="15.0" /></font>
                        </Button>
                        <Button
                            fx:id="suvFilterButton"
                            layoutX="448.0"
                            layoutY="18.0"
                            mnemonicParsing="false"
                            onAction="#filterBySUV"
